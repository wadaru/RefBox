diff -uprN rcll-refbox.origin/src/tools.view3/gamePhase.sh rcll-refbox.wataru/src/tools.view3/gamePhase.sh
--- rcll-refbox.origin/src/tools.view3/gamePhase.sh	1970-01-01 01:00:00.000000000 +0100
+++ rcll-refbox.wataru/src/tools.view3/gamePhase.sh	2020-02-24 10:05:21.533859222 +0100
@@ -0,0 +1,25 @@
+#!/bin/bash
+STATUSFILE="/home/robotino/rcll-refbox/view3/view3recv.gameState.txt"
+
+if [ -f $STATUSFILE ] ; then
+	read seq time phase state cyanPoint magentaPoint < $STATUSFILE
+	case $phase in
+	  0)
+		echo "pre_game"
+		;;
+	  10)
+		echo "setup"
+		;;
+	  20)
+		echo "exploration"
+		;;
+	  30)
+		echo "production"
+		;;
+	  40)
+		echo "post_game"
+		;;
+	esac
+else
+	echo "none"
+fi
