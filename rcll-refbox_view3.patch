+
+llsfrb::Configuration *config_;
+
+// for prepare-machine
+static bool quit = false;
+std::string machine_name_;
+std::string machine_type_;
+llsf_msgs::MachineSide bs_side_;
+llsf_msgs::BaseColor   bs_color_;
+int ds_order_id_;
+llsf_msgs::SSOp ss_op_;
